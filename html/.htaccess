RewriteEngine On
RewriteBase /EduarToDo/html

# Exclure les fichiers PHP sp√©cifiques de la redirection
RewriteCond %{REQUEST_FILENAME} -f
RewriteCond %{REQUEST_FILENAME} \.php$
RewriteRule ^ - [L]

# Si le fichier ou le dossier n'existe pas, redirige vers login
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ login.html [L]